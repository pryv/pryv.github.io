    "notes" : {
      "description" : "This is a generic type for all kinds of text notes a user may want to save. It includes subtypes such as standard text, html text, and more complex subtypes like Facebook events, etc.",
      "default" : {
        "type" : "string"
      },
      "sub" : {
        "facebook" : {
          "description" : "A Facebook event. ",
          "comments" : "start_time & end_time must be set into event's data & duration \nFacebook Event can be copied RAW as a value.. \nmap -> \n   fbEvent.name -> pryvEvent.comment\n   fbEvent.start_time -> pryvEvent.time\n   fbEvent.end_time - fbEvent.start_time -> pryvEvent.duration\n   \nTODO: what to do with videos & pictures?",
          "type" : "object",
          "properties" : {
            "venue" : {
              "type" : "object"
            },
            "id" : {
              "type" : "integer",
              "required" : "true"
            },
            "owner" : {
              "type" : "object",
              "properties" : {
                "name" : {
                  "type" : "string",
                  "required" : "true"
                },
                "id" : {
                  "type" : "integer",
                  "required" : "true"
                }
              }
            },
            "description" : {
              "type" : "string"
            },
            "location" : {
              "type" : "string"
            }
          }
        },
        "html" : {
          "description" : "An HTML-formatted note.",
          "maxLength" : "4194304"
        },
        "txt" : {
          "description" : "A plain-text note.",
          "maxLength" : "4194304"
        }
      }
    },
