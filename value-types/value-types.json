    "notes" : {
      "description" : "This is a generic type for all kinds of text notes a user may want to save. It includes subtypes such as standard text, html text, and more complex subtypes like Facebook posts, etc.",
      "default" : {
        "type" : "string"
      },
      "sub" : {
        "facebook" : {
          "description" : "A Facebook post. ",
          "comments" : "Facebook post can be copied RAW as a value. \nmap: \n   fbPost.message -> pryvEvent.description\n   fbPost.created_time -> pryvEvent.time\n TODO: what to do with videos & pictures?",
          "type" : "object",
          "properties" : {
