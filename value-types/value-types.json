              "properties": {
                "data": {
                  "type": "array",
                  "items":{
                    "type": "object",
                    "properties": {
                      "name": "string",
                      "id": "string"
                    }
                  }
                }  
              }
            },
            "message" : {
              "type" : "string"
            },
            "picture" : {
              "type" : "string"
            },
            "link" : {
